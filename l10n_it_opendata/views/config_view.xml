<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record id="view_opendata_configuration" model="ir.ui.view">
            <field name="name">OpenData Settings</field>
            <field name="model">opendata.config.settings</field>
            <field name="arch" type="xml">
                <form string="OpenData" version="7.0" class="oe_form_configuration">
                    <header>
                        <button string="Apply" type="object" name="execute" class="oe_highlight"/>
                        or
                        <button string="Cancel" type="object" name="cancel" class="oe_link"/>
                    </header>
                </form>
            </field>
        </record>

        <record id="action_opendata_configuration" model="ir.actions.act_window">
            <field name="name">OpenData</field>
            <field name="res_model">opendata.config.settings</field>
            <field name="view_mode">form</field>
            <field name="target">inline</field>
        </record>
        <menuitem id="menu_opendata_configuration" parent="base.menu_config"
            sequence="100" action="action_opendata_configuration"/>

	</data>
</openerp>
